{
  "name": "reminder",
  "private": true,
  "version": "0.1.23",
  "type": "module",
  "main": "dist-main/app.js",
  "author": "traeop",
  "description": "description",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:unit": "vitest src",
    "open:mac:prod": "./dist/mac/reminder.app/Contents/MacOS/reminder",
    "transpile:electron": "tsc --project src/main/tsconfig.json",
    "build:mac": "npm run transpile:electron && npm run build && electron-builder --mac --x64",
    "build:win": "npm run transpile:electron && npm run build && electron-builder --win --x64",
    "build:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/x-date-pickers": "^8.7.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.10.0",
    "compare-versions": "^6.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "electron-log": "^5.4.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lodash.isequal": "^4.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.51.1",
    "@types/lodash": "^4.17.16",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trae-op/reminder.git"
  }
}
